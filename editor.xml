<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="Consolas" />
    <option name="FONT_SCALE" value="1.25" />
  </component>
  <component name="EditorSettings">
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="notes">
      <option name="FOREGROUND" value="1f56c0" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <filter name="/**">
      <pattern index="2" />
    </filter>
    <filter name="//xx">
      <pattern index="2" />
    </filter>
  </component>
</application>